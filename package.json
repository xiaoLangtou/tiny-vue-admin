{
    "name": "tiny-vue-admin",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev:rsbuild": "rsbuild dev",
        "build:pro": "vue-tsc && rsbuild build --mode production",
        "build:pro:rsdoctor": "cross-env RSDOCTOR=true rsbuild build --mode production",
        "prettier": "prettier --write .",
        "lint": "oxlint . --fix -D correctness --ignore-path .oxlintignore  && eslint . --max-warnings=0",
        "lint:eslint": " eslint . --fix",
        "format": "prettier --write \"./**/*.{html,vue,ts,js,json,md}\"",
        "prepare": "husky install",
        "p": "plop"
    },
    "dependencies": {
        "@ant-design/icons-vue": "^7.0.1",
        "@vueuse/core": "^12.8.2",
        "alova": "^3.2.10",
        "animate.css": "^4.1.1",
        "ant-design-vue": "~4.2.6",
        "axios": "^1.8.2",
        "dayjs": "^1.11.13",
        "echarts": "^5.6.0",
        "echarts-liquidfill": "^3.1.0",
        "lucide-vue-next": "^0.475.0",
        "md5": "^2.3.0",
        "normalize.css": "^8.0.1",
        "nprogress": "^0.2.0",
        "pinia": "^3.0.1",
        "pinia-plugin-persistedstate": "^4.2.0",
        "query-string": "^9.1.1",
        "vue": "^3.5.13",
        "vue-draggable-next": "^2.2.1",
        "vue-draggable-plus": "^0.6.0",
        "vue-router": "^4.5.0",
        "vue3-json-viewer": "^2.2.2"
    },
    "devDependencies": {
        "@babel/core": "^7.26.10",
        "@commitlint/cli": "^17.8.1",
        "@commitlint/config-conventional": "^17.8.1",
        "@esbuild-kit/cjs-loader": "^2.4.4",
        "@rsbuild/core": "^1.2.18",
        "@rsbuild/plugin-babel": "^1.0.4",
        "@rsbuild/plugin-sass": "^1.2.2",
        "@rsbuild/plugin-vue": "^1.0.7",
        "@rsbuild/plugin-vue-jsx": "^1.1.0",
        "@tsconfig/node22": "^22.0.0",
        "@types/eslint-config-prettier": "^6.11.3",
        "@types/node": "^22.13.10",
        "@types/nprogress": "^0.2.3",
        "@types/vue3-json-viewer": "^2.2.0",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/eslint-config-typescript": "^14.5.0",
        "@vue/tsconfig": "^0.7.0",
        "autoprefixer": "^10.4.21",
        "commitizen": "^4.3.1",
        "commitlint-config-cz": "^0.13.3",
        "cross-env": "^7.0.3",
        "cz-customizable": "^7.4.0",
        "eslint": "^9.22.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-oxlint": "^0.15.14",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-vue": "~10.0.0",
        "globals": "^16.0.0",
        "husky": "^8.0.3",
        "husky-hook": "^1.3.0",
        "oxlint": "^0.15.14",
        "plop": "^4.0.1",
        "postcss": "^8.5.3",
        "prettier": "3.5.3",
        "rollup-plugin-visualizer": "^5.14.0",
        "sass": "^1.85.1",
        "tailwindcss": "^3.4.17",
        "typescript": "~5.8.2",
        "typescript-eslint": "^8.26.1",
        "unplugin-auto-import": "^19.1.1",
        "unplugin-vue-components": "^28.4.1",
        "vue-tsc": "^2.2.8"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
        }
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-customizable"
        }
    }
}
